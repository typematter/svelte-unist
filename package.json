{
	"name": "@accuser/svelte-unist",
	"version": "0.5.0",
	"private": false,
	"license": "MIT",
	"author": {
		"email": "mhgibbons@me.com",
		"name": "Matthew Gibbons",
		"url": "https://github.com/accuser"
	},
	"keywords": [
		"unist",
		"svelte",
		"sveltekit"
	],
	"homepage": "https://github.com/accuser/svelte-unist",
	"bugs": {
		"url": "https://github.com/accuser/svelte-unist/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/accuser/svelte-unist.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "pnpm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.39.11"
	},
	"dependencies": {
		"@accuser/unist-util-type-guards": "^1.1.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/kit": "^2.46.4",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@types/eslint": "^9.6.1",
		"@types/node": "^24.7.0",
		"@types/unist": "^3.0.3",
		"@typescript-eslint/eslint-plugin": "^8.46.0",
		"@typescript-eslint/parser": "^8.46.0",
		"eslint": "^9.37.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"jsdom": "^27.0.0",
		"pnpm": "^10.18.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.14",
		"svelte": "^5.39.11",
		"svelte-check": "^4.3.3",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.0",
		"unist-builder": "^4.0.0",
		"vite": "^7.1.9",
		"vitest": "^3.2.4"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
